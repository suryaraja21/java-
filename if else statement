import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        Scanner s= new Scanner(System.in);
        System.out.println("Enter your age: ");
        int a=s.nextInt();
        if(a>=18){
            System.out.println("You are eligible!");
        }else{
            System.out.println("You are not eligible");
        }
    }
}
